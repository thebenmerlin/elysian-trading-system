# Elysian Trading System - Frontend Environment Variables
NEXT_PUBLIC_API_URL=https://elysian-backend-bd3o.onrender.com
NEXT_PUBLIC_API_KEY=elysian-demo-key

# Elysian Trading System - Dual-Market Configuration

# Server Configuration
NODE_ENV=development
PORT=4000

# Database
DATABASE_URL=postgres://user:pass@localhost:5432/elysian

# Trading Configuration - EQUITY
RUNNER_TICKERS=AAPL,MSFT,GOOGL,NVDA,TSLA
EQUITY_RUN_INTERVAL_MINUTES=15
RUN_INTERVAL_MINUTES=15

# Trading Configuration - CRYPTO
CRYPTO_TICKERS=BTCUSDT,ETHUSDT,ADAUSDT,DOTUSDT,LINKUSDT
CRYPTO_RUN_INTERVAL_MINUTES=5
CRYPTO_MAX_DAILY_RUNS=288
CRYPTO_REFLECTION_EVERY=24
CRYPTO_REPORT_EVERY=48

# System Configuration
ELYSIAN_LIVE=false
INITIAL_CASH=100000
AUTO_START_RUNNER=true
ENABLE_AI_ANALYSIS=true
LOG_LEVEL=info

# API Keys (Optional)
HF_API_KEY=
ELYSIAN_API_KEY=your_demo_key

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:4000
NEXT_PUBLIC_API_KEY=your_demo_key

# Dual-Market Features
ENABLE_CRYPTO_TRADING=true
ENABLE_DUAL_MARKET_MODE=true
